<div class="card">
    <div class="wave"></div>
    <h3 class="card_header">Sign Up</h3>
    <form id="signup-form" class="card_body signup-form">
      <label for="username-signup" class="form-label">Username</label>
      <input id="username-signup" type="text" class="form_input" />
      <label for="password-signup" class="form-label">Password</label>
      <input id="password-signup" type="password" class="form_input" />
      <button type="submit" class="btn">Signup!</button>
      <a class="login" href="/login">Log in instead</a>
    </form>
</div>
<script src="/LoginJavaScript/signup.js"></script>
